{% extends 'BackBundle::base.html.twig' %}
{% block body %}
<div class="wrapper kode-header-class-3">
    <!--BANNER START-->
    <div class="kode-banner">
        <img src="{{ asset("assets/front/images/banner.png") }}" alt="">
    </div>
</div>
    <section class="kode-membership">
        <div class="container">
            <form action="{{ path('fos_user_resetting_send_email') }}" method="POST" class="fos_user_resetting_request">
                <div class="kode-navigation">
                    <ul>
                        <li>
                            <label for="username">{{ 'resetting.request.username'|trans }}></label>
                        </li>
                    </ul>
                    <ul>
                        <li>
                            <input type="text" id="username" name="username"  placeholder="Your email" required="required" />
                        </li>
                    </ul>
                    <ul>
                        <li>
                            <input type="submit" value="{{ 'resetting.request.submit'|trans }}" />
                        </li>
                    </ul>
                </div>
            </form>
        </div>
    </section>

{% endblock %}
