<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>Gutenbook - The way books make it real</title>


    <link href="{{ asset('assets/front/css/style.css') }}" rel="stylesheet">
    <link href="{{ asset('assets/front/css/themetypo.css') }}" rel="stylesheet">
    <link href="{{ asset('assets/front/css/bootstrap.css') }}" rel="stylesheet">
    <link href="{{ asset('assets/front/css/color.css') }}" rel="stylesheet">
    <link href="{{ asset('assets/front/css/font-awesome.min.css') }}" rel="stylesheet">
    <link href="{{ asset('assets/front/css/jquery.bxslider.css') }}" rel="stylesheet">
    <link href="{{ asset('assets/front/css/bootstrap-slider.css') }}" rel="stylesheet">
    <link href="{{ asset('assets/front/css/widget.css') }}" rel="stylesheet">
    <link href="{{ asset('assets/front/css/owl.carousel.css') }}" rel="stylesheet">
    <link href="{{ asset('assets/front/css/responsive.css') }}" rel="stylesheet">
    <link href="{{ asset('assets/front/js/dl-menu/component.css') }}" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="{{ asset('assets/front/css/bookblock.css') }}" />
