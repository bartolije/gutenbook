    <link href="{{ asset("assets/front/css/jquery-confirm.min.css") }}" rel="stylesheet">

    <link href="{{ asset('assets/front/css/custom.css') }}" rel="stylesheet">
    <link href="{{ asset('assets/front/css/form.css') }}" rel="stylesheet">

    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    <div id="loader-wrapper">
        <div id="loader"></div>
        <div class="loader-section section-left"></div>
        <div class="loader-section section-right"></div>
    </div>

    <div class="wrapper kode-header-class-3">

        {% include "FrontBundle:UI:menu.html.twig" %}

        {% include "@Front/Default/caroussel.html.twig" %}

        {{ render(controller( 'FrontBundle:Default:search' )) }}

        <div class="kode-content">
            {% block body %}
            {% endblock %}
        </div>

        {% include "FrontBundle:UI:footer.html.twig" %}

    </div>
    {% block javascript %}

    <script src="{{ asset('assets/front/js/jquery.min.js') }}"></script>
    <script src="{{ asset('assets/front/js/modernizr.custom.js') }}"></script>
    <script src="{{ asset('assets/front/js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('assets/front/js/jquery.bxslider.min.js') }}"></script>
    <script src="{{ asset('assets/front/js/bootstrap-slider.js') }}"></script>
    <script src="{{ asset('assets/front/js/waypoints.min.js') }}"></script>
    <script src="{{ asset('assets/front/js/jquery.counterup.min.js') }}"></script>
    <script src="{{ asset('assets/front/js/owl.carousel.js') }}"></script>
    <script src="{{ asset('assets/front/js/dl-menu/jquery.dlmenu.js') }}"></script>
    <script type="text/javascript" src="{{ asset('assets/front/lib/hash.js') }}"></script>
    <script type="text/javascript" src="{{ asset('assets/front/lib/booklet-lib.js') }}"></script>
    <script src="{{ asset('assets/front/js/jquerypp.custom.js') }}"></script>
    <script src="{{ asset('assets/front/js/jquery.bookblock.js') }}"></script>
    <script src="{{ asset('assets/front/js/functions.js') }}"></script>
    <script src='http://codepen.io/assets/libs/fullpage/jquery.js'></script>
    <script src="{{ asset("assets/front/js/jquery-confirm.min.js") }}"></script>
    <script src="{{ asset('assets/front/js/profile.js') }}"></script>
    <script>
        $('a.delete').confirm({
            title:"Suppression",
            content: "Voulez-vous supprimer cette élément ?",
            buttons: {
                confirmer: function(){
                    location.href = this.$target.attr('href');
                },
                annuler: function(){

                }
            }
        });
    </script>
    {% endblock %}

</body>
</html>